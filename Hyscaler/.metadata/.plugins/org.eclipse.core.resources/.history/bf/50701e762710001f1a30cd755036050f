// firts request to server to create order

 const paymentStart = ()=>{
	 
	 
	 $.ajax(
		 {
			 url:'/Event_Management_System/paymentpage',
			 contentType:'application/json',
			 type:'POST',
			 dataType:'json',
			 success: function(response){
				console.log(response)
			 
			 },
			 error:function(error){
				 console.log(error)
				 alert("Something went Wrong")
			 }
			 
		 }
	 )
	 
 };